package main
import(
  //"fmt"
  //"log"
  "math/rand"
  "github.com/gin-gonic/gin"
  //"net/http"
  //"strings"
  "time"
)

func init(){
  rand.Seed(time.Now().UnixNano())
}

func main(){
  r := gin.Default()
  r.GET("/status", func(c *gin.Context){
    c.JSON(200, gin.H{"status":"Running",})
  })
}
